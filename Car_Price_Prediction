        "print('Train RMSE: ', rmse(y_train, y_pred))\n",
        "\n",
        "X_val = prepare_X(df_val)\n",
        "y_pred = w_0 + X_val.dot(w)\n",
        "print('Validation RMSE: ', rmse(y_val, y_pred))"
